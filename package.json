{
  "name": "@hp4k1h5/terminordle",
  "version": "0.0.13",
  "license": "MIT",
  "scripts": {
    "help": "yarn dev --help",
    "play": "yarn dev play",
    "join": "yarn dev join",
    "serve": "yarn dev serve",
    "test": "mocha -r ts-node/register 'tests/**/*.*'",
    "dev": "yarn build && node dist/src/cli/index.js",
    "build": "rm -rf dist/ && tsc && cp -r dist_data dist",
    "pub": "yarn build && npm publish --access public"
  },
  "bin": {
    "terminordle": "dist/src/cli/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HP4k1h5/terminordle.git"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "commander": "^9.0.0",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/chalk": "^2.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.17",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "chai": "^4.3.6",
    "eslint": "^8.9.0",
    "eslint-plugin-mocha": "^10.0.3",
    "mocha": "^9.2.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "typescript-strict-plugin": "^2.0.0-beta.1"
  },
  "files": [
    "src/**/*",
    "dist/**/*.*"
  ],
  "keywords": [
    "wordle",
    "multiplayer",
    "game",
    "typescript",
    "websockets",
    "terminal",
    "cli",
    "tui"
  ]
}
